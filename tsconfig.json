{
  "extends": "@tsconfig/node14/tsconfig.json",
  "compilerOptions": {
    "lib": ["es2020", "DOM"],
    "declaration": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "importsNotUsedAsValues": "error",
    "strict": true,
    "outDir": "./dist"
  },
  "include": ["src/**/*"]
}
